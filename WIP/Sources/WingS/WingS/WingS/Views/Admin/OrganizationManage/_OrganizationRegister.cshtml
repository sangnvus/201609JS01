    <body>
    @Html.Partial("../Admin/_TopSideNavigation")
    <!-- begin MAIN PAGE CONTENT -->
    <div id="page-wrapper">
        <div class="page-content">
            <!-- begin PAGE TITLE ROW -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="page-title">
                        <h1>
                            Tổ chức
                        </h1>
                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-dashboard"></i>  <a href="/Admin/#/DashBoard">Thống kê</a>
                            </li>
                            <li class="active">Danh sách tổ chức đăng ký</li>
                        </ol>
                    </div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <!-- end PAGE TITLE ROW -->
            <!-- start DASHBOARD CIRCLE TILES -->
            @Html.Partial("../Admin/OrganizationManage/_OrganizationCircleTiles")
            <!-- end DASHBOARD CIRCLE TILES -->
            <!-- begin ADVANCED TABLES ROW -->
            <div class="row">

                <div class="col-lg-12">

                    <div class="portlet portlet-default">
                        <div class="portlet-heading">
                            <div class="portlet-title">
                                <h4>Danh sách tổ chức đăng ký</h4>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="portlet-body">
                            <div class="table-responsive">
                                <table datatable="ng" id="example-table" class="table table-striped table-bordered table-hover table-green">
                                    <thead>
                                        <tr>
                                            <th style="width: 20px;">STT</th>
                                            <th style="">Người đăng ký </th>
                                            <th style="">Ngày đăng ký</th>
                                            <th style="">Tên tổ chức</th>
                                            <th style="">Địa chỉ Email</th>
                                            <th>Trạng thái</th>
                                            <th>Hành động</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="odd gradeX" ng-repeat="item in allOrgWaitAccept">
                                            <td>{{ $index + 1 }}</td>
                                            <td class="nameAdmin">
                                                <a class="pull-left" href="@Url.Content("/Admin/#/UserProfile/{{item.OrganizationId}}")">
                                                    <p> {{item.Creator.UserName}}</p>
                                                </a>
                                            </td>
                                            <td>{{item.CreatedDate}}</td>
                                            <td>{{item.OrganizationName}}</td>
                                            <td>{{item.Email}}</td>
                                            <td>                                   
                                                <button class="btn btn-warning" style="width: 125px" ng-if="!item.IsVerify" onclick="return false">Chờ phê duyệt</button>
                                                <button class="btn btn-primary" style="width: 125px" ng-if="item.IsVerify && item.IsActive" onclick="return false">Đồng ý</button>
                                                <button class="btn btn-danger" style="width: 125px" ng-if="item.!IsActive" onclick="return false">Từ chối</button>
                                            </td>
                                            <td>
                                                <button class="btn btn-primary" ng-click="AcceptOrg($index,item.OrganizationId)">Đồng ý</button>
                                                <button class="btn btn-danger" ng-click="cancelCreateOrg($index,item.OrganizationId)">Từ chối</button>
                                            </td>

                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
                        </div>
                        <!-- /.portlet-body -->
                    </div>
                    <!-- /.portlet -->

                </div>
                <!-- /.col-lg-12 -->

            </div>
        </div>
        <!-- /.page-content -->
    </div>
    <!-- /#page-wrapper -->
    <!-- end MAIN PAGE CONTENT -->
    <!-- HISRC Retina Images -->
    <script src="~/Content/Admin/js/plugins/hisrc/hisrc.js"></script>
    <!-- THEME SCRIPTS -->
    <script src="~/Content/Admin/js/flex.js"></script>
</body>
